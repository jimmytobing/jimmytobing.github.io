{
  "$jason": {
    "head": {
      "title": "Corono",
      "data": {"tmp": ""},
      "actions": {
			
				"$load": {
					"trigger": "script_load"
				},
				
				"script_load": {
          "type": "$script.include",
          "options": {
            "items": [
              {
                "text": "var randomcolor = function() { return '#'+Math.floor(Math.random()*16777215).toString(16); }",
								"text": "var jimdebug = function(obj) { alert(JSON.stringify(obj, null, 2)); return obj; }"
              }
            ]
          },
          "success": {
            "trigger": "navigate_tmp"
          }
        },
				
				"$pull": {
          "trigger": "draw"
        },
				
				"draw": {
          "type": "$render",
          "options": {
            "data": {
              "color": "{{randomcolor()}}",
              "background": "{{randomcolor()}}"
            }
          }
        },
				
				"get_location": {
					"type": "$geo.get",
					"options": {
						"distance": "1000"
					},
					"success": {
						"type": "$href",
						"options": {
							"url": "http://maps.google.com/maps?q=&layer=c&cbll={{$jason.coord}}&cbp=11,0,0,0,0",
							"view": "App"
						}
					}
				},
				
				"get_addrbook" :{
          "type": "$util.addressbook",
          "success": {
            "trigger": "alerting_get"
          }
				},
				
				"alerting_get": {
					"type": "$util.alert",
					"options": {
						"title": "alerting $get",
						"description": "{{JSON.stringify($get, null, 2)}}"
					},
					"success": {
						"type": "$render"
					}				
				},
				
				"set_global": {
						"type": "$global.set",
						"options": {
							"serveraddr": "https://jimmytobing.github.io/corono"
						},
						"success": {
								"trigger": "cek_user"
						}
				},
				
				"cek_user": [
					{
						"{{#if $global && 'user' in $global}}": 
							{
								"type": "$render"											
							}
					},{
						"{{#else}}":
							{
								"trigger": "navigate"
							}
					}
				],
				
				"alerting_global": {								
					"type": "$util.alert",
					"options": {
						"title": "alerting $global langsung",
						"description": "{{JSON.stringify($global, null, 2)}}"
					},
					"success": {
						"type": "$render"
					}				
				},
				
				"navigate_tmp": {
					"type": "$href",
					"options": {
							"url": "https://jimmytobing.github.io/corono/sehat.json",
							"options": {
									"paramku": "{{$jason.paramku}}",
									"urlku": "{{$jason.url}}"
							}
					}								
				},
				
				"navigate": {
					"type": "$href",
					"options": {
							"url": "https://jimmytobing.github.io/corono/hellocorono.json",
							"options": {
									"paramku": "{{$jason.paramku}}",
									"urlku": "{{$jason.url}}"
							}
					}								
				}

      },
      "templates": {
        "body": {
					"style": {
						"border": "none"
					},
          "sections": [{
						"items": [
							{
								"type": "vertical",
								"style": {
									"padding": "5%"
								},
								"components": [
									{
										"type": "space",
										"style": {
											"height": "15%"
										}
									},
									{
										"type": "label",
										"text": "Corono",
										"style": {
											"align": "center",
											"size": "44",
											"color": "#000000"
										}
									},
									{
										"type": "label",
										"text": "Selamat Datang {{$global.user.nama}}",
										"style": {
											"align": "center",
											"size": "20",
											"color": "#000000"
										}
									}
									
									
								]
							},							
							{
								"type": "label",
								"text": "Lanjutkan >",
								"style": {
									"align": "center",
									"size": "20",
									"color": "#007aff"
								},
								"href": {
									"url": "https://jimmytobing.github.io/corono/corono.json"
								}
							},
							{
								"type": "space",
								"style": {
									"height": "31%"
								}
							},						
						
							{
								"type": "label",
								"text": "< Keluar",
								"style": {
									"align": "center",
									"color": "#007aff"
								},
								"href": {
									"url": "https://jimmytobing.github.io/corono/registercorono.json"
								}
							}
							
									
							
						]
					}]
        }
      }
    }
  }
}
